<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./styles/styles.css">
    {{!-- <script src="../public/js/index.js"></script> --}}
    <title>codeblog</title>
</head>
<div class="header" id="header"></div>
<div class="row">
    <div class="leftcolumn">
      {{#each this}}
      <div class="card">
        <h2 class="single-poost-title">{{this.title}}</h2>
        <h5 class="single-post-meta"> {{this.publish_date}} <span class="author-by"> {{this.author}} </span></h5>
        <img src="{{this.header_image}}" alt="header image" class="fakeimg single-post-image"></img>
        <P>{{this.content}}</P>
      </div>
        {{/each}}
    </div>
  <div class="rightcolumn" id="sidebar"></div>
</div>
</div>

<div class="footer" id="footer"></div>


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="./js/index.js"></script>

