<div class="header" id="header">
</div>
<div class="row">
  <div class="leftcolumn">

    <div class="card">
      <ul class="article_list">
        {{#each blogs}}
        <li>
          <img src="{{header_image}}" alt="header image" class="fakeimg single-post-image"></img>
          <div>
            <a href="/content/{{post_id}}" class="article_list_title">{{title}}</a>

          </div>
        </li>
        {{/each}}
      </ul>
    </div>

  </div>
  <div class="rightcolumn" id="sidebar"></div>
</div>

<div class="footer" id="footer"></div>

<script src="/js/index.js"></script>
